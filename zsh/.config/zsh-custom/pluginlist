# Build plugin list conditionally based on available tools
plugins=(
  aliases
  azure
  dotnet
  gh
  git
  git-escape-magic
  gpg-agent
  kubectl
  ssh-agent
  uv
  vault
  vi-mode
)

# Add fzf plugin only if FZF_BASE is set
[ -n "$FZF_BASE" ] && plugins+=(fzf)

# Add zoxide plugin only if zoxide is installed
command -v zoxide >/dev/null 2>&1 && plugins+=(zoxide)
